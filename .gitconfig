[user]
	name = 
	email = 
[color]
        branch = auto
        diff = auto
        status = auto
[color "branch"]
        current = yellow reverse
        local = yellow
        remote = green
[color "diff"]
        meta = green
        frag = magenta bold
        old = red
        new = cyan
[color "status"]
        added = yellow
        changed = green
        untracked = cyan

[push]
        default = current
[alias]
        st = status
        ci = commit
        co = checkout

[core]
        editor = vim

